<!-- Skitarii ranger hood -->
	<!-- Skitarii hood stripe -->
	<Core40k.ExtraDecorationDef>
		<defName>Seg_COTO_Deco_SkitariiRangerHoodGearStripe</defName>
		<label>Cog-edged stripe</label>
		<description>...</description>
		<iconPath>UI/Decoration/Icons/SkitariiRangerHood/Seg_COTO_Deco_SkitariiRangerHood_GearStripe</iconPath>
		<drawnTextureIconPath>Things/Decoration/SkitariiRangerHood/Seg_COTO_SkitariiRangerHood_GearStripe</drawnTextureIconPath>
		<drawSize>(0.5, 0.5)</drawSize>
		<sortOrder>1</sortOrder>
		<isHelmetDecoration>true</isHelmetDecoration>

		<defaultShowRotation>
			<li>East</li>
			<li>South</li>
			<li>West</li>
		</defaultShowRotation>

		<appliesTo>
			<li>Seg_COTO_Apparel_SkitariiRangerHood</li>
		</appliesTo>

		<drawData>
			<dataNorth>
				<layer>77</layer>
				<flip>true</flip>
			</dataNorth>
			<dataSouth>
				<layer>81</layer>
			</dataSouth>
			<dataEast>
				<layer>75</layer>
			</dataEast>
			<dataWest>
				<layer>75</layer>
			</dataWest>
		</drawData>

		<shaderType>Cutout</shaderType>
		<defaultColour>(255,255,255)</defaultColour>

		<colourable>true</colourable>

		<hasArmorColourPaletteOption>true</hasArmorColourPaletteOption>
		<useArmorColourAsDefault>false</useArmorColourAsDefault>
	</Core40k.ExtraDecorationDef>

