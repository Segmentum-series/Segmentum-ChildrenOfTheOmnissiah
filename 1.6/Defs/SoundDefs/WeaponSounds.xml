<Defs>
	<!-- Stubber sound -->
	<SoundDef>
		<defName>Seg_COTO_Stubber</defName>  
		<context>MapOnly</context>  
		<maxSimultaneous>99</maxSimultaneous>  
		<subSounds>
			<li>
