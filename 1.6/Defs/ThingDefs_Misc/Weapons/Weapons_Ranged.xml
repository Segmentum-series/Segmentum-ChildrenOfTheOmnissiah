			<li>Seg_COTO_WeaponRangedMidTier</li>
		</tradeTags>
	</ThingDef>
	
	<!-- Arc Pistol -->
	<ThingDef ParentName="Seg_COTO_ArcWeaponBase">
		<defName>Seg_COTO_Gun_ArcPistol</defName>
		<label>arc pistol</label>
		<description>A compact arc‑energy sidearm that fires a thin, tracking beam of searing energy.</description>

		<graphicData>
			<texPath>Things/Weapon/Seg_COTO_Ranged_ArcPistol</texPath>
			<shaderType>BEWH_CutoutThreeColor</shaderType>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.2</drawSize>
		</graphicData>

		<statBases>
			<Mass>2.5</Mass>
			<EquipDelay>0.5</EquipDelay>
			<RangedWeapon_Cooldown>1.0</RangedWeapon_Cooldown>
			<AccuracyTouch>0.80</AccuracyTouch>
			<AccuracyShort>0.90</AccuracyShort>
			<AccuracyMedium>0.80</AccuracyMedium>
			<AccuracyLong>0.55</AccuracyLong>
			<WorkToMake>6000</WorkToMake>
			<MarketValue>500</MarketValue>
		</statBases>

		<verbs>
			<li>
			<verbClass>Verb_Shoot</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>Seg_COTO_ArcPistol_Beam</defaultProjectile>
			<soundCast>Seg_COTO_ArcSound</soundCast>
			<warmupTime>1</warmupTime>
			<range>20.0</range>
			<burstShotCount>1</burstShotCount>
			<ticksBetweenBurstShots>20</ticksBetweenBurstShots>
			<muzzleFlashScale>10</muzzleFlashScale>
			</li>
		</verbs>

		<costList>
			<Steel>40</Steel>
			<ComponentIndustrial>3</ComponentIndustrial>
		</costList>

		<recipeMaker>
			<researchPrerequisites>
				<li>Seg_COTO_TechGuard</li>
			</researchPrerequisites>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<skillRequirements>
			<Crafting>6</Crafting>
			</skillRequirements>
		</recipeMaker>
		<relicChance MayRequire="Ludeon.RimWorld.Ideology">0</relicChance>
	</ThingDef>

